generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "mongodb"
  url      = env("MONGO_DB_URL")
}


model Translation {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  lang      String  
  key       String 
  category  String  
  value     String   
  updatedAt DateTime @updatedAt
}

